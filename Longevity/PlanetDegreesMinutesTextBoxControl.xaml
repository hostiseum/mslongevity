<UserControl x:Class="Longevity.PlanetDegreesMinutesTextBoxControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Longevity"
             mc:Ignorable="d" 
             d:DesignHeight="15" d:DesignWidth="120">
    <StackPanel Orientation="Horizontal" Margin="1,1,1,1">
        <TextBlock Text="Planet"></TextBlock>
        <TextBox Name="Degree" Width="30" Margin="3,0,0,0"></TextBox>
        <TextBox Name="Minute" Width="30" Margin="3,0,0,0"></TextBox>
    </StackPanel>
</UserControl>
